meta {
  name: getMembers
  type: http
  seq: 1
}

get {
  url: {{url}}/members
  body: none
  auth: none
}

tests {
  test("Response header type should be application/json", function() {
    const contentType = res.getHeader("content-type");
    expect(contentType).to.be.equal("application/json")
  })
  
  test("Should include members", function() {
    const data = res.getBody();
    expect(data.members.length).to.be.not.null;
  });
  
  test("Members should have an hash value", function() {
    const data = res.getBody();
    expect(data.members.hash).to.be.not.null;
  });
  
  test("Members should have an name value", function() {
    const data = res.getBody();
    expect(data.members.name).to.be.not.null;
  });
  
  test("Members should have an position value", function() {
    const data = res.getBody();
    expect(data.members.position).to.be.not.null;
  });
  
  test("Members should have an avatar value", function() {
    const data = res.getBody();
    expect(data.members.avatar).to.be.not.null;
  });
  
  test("Members should have an linkedin value", function() {
    const data = res.getBody();
    expect(data.members.linkedin).to.be.not.null;
  });
  
}
